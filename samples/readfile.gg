#!/bin/gg
#

# Arguments users :: as definition
stdout:	1
# ::$ allows recover the number of arguments passed to the program or function
# It can be recovered in gdb by using 
# (gdb) print *((int*)($rsp))
args::$
msg:	arguments: args
write	args
write
bin name::	0
file name::	1
read file:	{
	file::	1
	file name:	samples/sample-code.gg
	write	stdout	file name
	file contents:<=	file name
	#write	stdout	file contents
	ret
}
read file	sample-code.gg

no error:	0
exit	no error
